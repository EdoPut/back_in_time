{
    "manifest_version": 2,
    "name": "Tab origin",
    "description": "Return to the page from which you opened this tab.",
    "version": "0.4.1",
    "permissions": [
        "webNavigation",
        "storage",
        "tabs"
    ],
    "page_action": {
        "browser_style": true,
        "default_title": "Open origin tab",
        "default_icon": "time_machine.png"
    },
    "background": {
        "scripts": [ "remember.js" ]
    }
}
